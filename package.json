{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "tailscale",
  "title": "Tailscale",
  "description": "List devices in your Tailnet and copy the IPv4, IPv6, or web address to your clipboard.",
  "icon": "command-icon.png",
  "author": "samlinville",
  "categories": [
    "Developer Tools",
    "Web",
    "Security"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "List Tailnet devices",
      "description": "Template for showing a static list and a detail view",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.31.0",
    "rss-parser": "^3.12.0"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "react-devtools": "^4.19.2",
    "run-applescript": "^6.0.0",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
